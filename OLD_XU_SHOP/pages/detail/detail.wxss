page {
  background:#e5e5e5;
  padding-bottom:10px;
  color:black;
  font-size: 14px;
}
swiper {
  background:#fff;
  padding-bottom:10px;
  border-bottom: 2px #f5f5f5 solid;

}
swiper-item {
  display:flex;
  justify-content:center;
}
.slide-image {
  width:90%;
  height: 100%;
}
.produceName {
  background:#fff;
  margin-top:10px;
  height:60px;
  line-height:80px;
  color:black;
}
.colorClassify{
  display: flex;
  justify-content: space-around;
  align-items:center;
  background:#fff;
  height:60px;
  border-top:1px solid #f5f5f5;
}
.colorClassify view {
  width:45px;
  height: 30px;
  border-radius:15px;
  text-align:center;
}
.colorClassify .colorText {
  width:60px;
}
.produceIntroText {
  height:60px;
  line-height:60px;
  border-top:1px solid #f5f5f5;
  border-bottom:1px solid #f5f5f5;
  color:black;

}
.produceIntro .produceDetail {
  padding:10px 40px;
}
.picShow image {
  width:100%;
}
.pic {
  height: 60px;
  line-height: 60px;
  border-top: 1px solid #f5f5f5;
}
.line {
  line-height:50px;
  text-align:center;
  color:#e5e5e5;
  font-size:14px;
}